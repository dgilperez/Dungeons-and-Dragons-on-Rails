<%= simple_form_for(@insider_datum) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :original_id %>
    <%= f.input :name %>
    <%= f.input :html %>
    <%= f.input :data_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
